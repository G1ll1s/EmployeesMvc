@model Employee[]


<h1>Anställda:</h1>
@foreach (var employee in Model)
{
    <li>
        <a asp-controller="employees" asp-action="details" asp-route-id="@employee.Id">
            @employee.Name - @employee.Email
        </a>

    </li>
}

    <a asp-controller="employees" asp-action="create">Lägg till anställd</a>

